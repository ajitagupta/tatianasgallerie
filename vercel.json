{
    "version": 2,
    "name": "tatianas-gallerie",
    "builds": [
        {
            "src": "api/process-payment.js",
            "use": "@vercel/node"
        },
        {
            "src": "docs/**",
            "use": "@vercel/static"
        },
        {
            "src": "images/**",
            "use": "@vercel/static"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "paintings*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/process-payment",
            "dest": "api/process-payment.js"
        },
        {
            "src": "/docs/(.*)",
            "dest": "/docs/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/images/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "STRIPE_SECRET_KEY": "STRIPE_SECRET_KEY"
    },
    "github": {
        "silent": true
    }
}